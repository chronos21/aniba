<%- include('./header') %>
<div class="ui container">
    <div class="ui relaxed selection list" style="margin-bottom: 30px;">
        <% data.forEach(function(item){ %>
        <div class="item">
            <a class="mini ui icon button" 
            target="_blank" href="/<%= item.href %>?download=true"   
            style="float: right;">
                <i class="download icon"></i>
            </a>
            <div class="content">
                <a class="header" href="/<%= item.href %>" target="_blank">
                    <%= item.title %>
                </a>
                <div class="description"><%= item.time %></div>
            </div>
           
        </div>
        <% }); %>
    </div>
</div>
<%- include('./footer') %>