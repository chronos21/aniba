<%- include('./header') %>
    <div class="ui container">
        <div class="ui relaxed selection list" style="margin-bottom: 30px;">
            <h3>results for: "<%= q %>"</h3>
        <% data.forEach(function(item){ %>
            <div class="item">
                <div class="content">
                    <a class="header" href="<%= item.href %>">
                        <%= item.title %>
                    </a>
                    <div class="description"><%= item.episodes %> | <%= item.status %></div>
                </div>
            </div>
        <% }); %>
    </div>
<%- include('./footer') %>

